---
import Layout from '~/layouts/PageLayout.astro';
import Stats from '~/components/widgets/Stats.astro';
import Features from '~/components/widgets/Features.astro';
import Features3 from '~/components/widgets/Features3.astro';

const metadata = {
  title: 'Survey',
  ignoreTitleTemplate: true,
};
---

<Layout metadata={metadata}>

<div class="mb-8 md:mx-auto md:mb-12 text-center max-w-3xl">
<p class="mt-4 text-muted text-xl"><br />Please take a few minutes to complete the following survey. Your feedback will help us serve you better.</p>
</div>

  <!-- Hero Widget ******************* -->
  <iframe data-tally-src="https://tally.so/embed/mVJ1zl?hideTitle=1&dynamicHeight=1" loading="lazy" width="100%" height="386" frameborder="0" marginheight="0" marginwidth="0" title="Muslim Outdoor Adventures Group Survey"></iframe><script>var d=document,w="https://tally.so/widgets/embed.js",v=function(){"undefined"!=typeof Tally?Tally.loadEmbeds():d.querySelectorAll("iframe[data-tally-src]:not([src])").forEach((function(e){e.src=e.dataset.tallySrc}))};if("undefined"!=typeof Tally)v();else if(d.querySelector('script[src="'+w+'"]')==null){var s=d.createElement("script");s.src=w,s.onload=v,s.onerror=v,d.body.appendChild(s);}</script>


</Layout>